<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="524.0" prefWidth="776.0" style="-fx-background-color: #ff8a05;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cadastrofun.Controller.FuncionarioController">
   <children>
      <VBox fillWidth="false" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="680.0" style="-fx-background-color: #fff; -fx-background-radius: 20px;">
         <children>
            <Label alignment="CENTER" contentDisplay="TOP" prefHeight="46.0" prefWidth="324.0" text="Cadastre seu funcionário" textAlignment="CENTER" textFill="#d37c09">
               <font>
                  <Font name="Arial Rounded MT Bold" size="25.0" />
               </font>
               <VBox.margin>
                  <Insets left="170.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="nomeField" prefHeight="37.0" prefWidth="309.0" promptText="Nome" style="-fx-background-radius: 20px;">
               <VBox.margin>
                  <Insets left="180.0" top="20.0" />
               </VBox.margin></TextField>
            <TextField fx:id="telefoneField" prefHeight="37.0" prefWidth="309.0" promptText="Telefone" style="-fx-background-radius: 20px;" >
               <VBox.margin>
                  <Insets left="180.0" right="100.0" top="30.0" />
               </VBox.margin></TextField>
            <TextField fx:id="enderecoField" prefHeight="37.0" prefWidth="309.0" promptText="Endereço" style="-fx-background-radius: 20px;">
               <VBox.margin>
                  <Insets left="180.0" right="100.0" top="30.0" />
               </VBox.margin></TextField>
            <TextField fx:id="cargoField" prefHeight="37.0" prefWidth="309.0" promptText="Cargo" style="-fx-background-radius: 20px;">
               <VBox.margin>
                  <Insets left="180.0" right="100.0" top="30.0" />
               </VBox.margin></TextField>
            <Button mnemonicParsing="false" fx:id="cadastrarButton"  onAction="#cadastrarFuncionario" prefHeight="37.0" prefWidth="160.0" style="-fx-background-color: #ff8a05;" text="Cadastrar" textFill="WHITE">
               <VBox.margin>
                  <Insets left="250.0" top="40.0" />
               </VBox.margin></Button>
         </children>
         <VBox.margin>
            <Insets left="60.0" right="50.0" />
         </VBox.margin>
      </VBox>
   </children>
   <padding>
      <javafx.geometry.Insets right="10.0" />
   </padding>
</VBox>
